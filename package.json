{
  "name": "google-ads-api",
  "version": "0.1.0",
  "description": "Google Ads API Client Library for JavaScript",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.test\\.ts$"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Opteo/google-ads-api.git"
  },
  "keywords": [],
  "author": "Opteo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Opteo/google-ads-api/issues"
  },
  "homepage": "https://github.com/Opteo/google-ads-api#readme",
  "devDependencies": {
    "@types/jest": "^23.1.6",
    "jest": "^23.4.1",
    "ts-jest": "^23.0.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.112",
    "@types/request": "^2.47.1"
  }
}
